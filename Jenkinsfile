pipeline
{
    agent {label "kosipipelineexecutor"}
    stages
    {
        stage('Prepare the code')
        {
            steps
            {
                echo "${STAGE_NAME}"                                
            }
        }
        stage('Build the code')
        {
            steps
            {
                echo "${STAGE_NAME}"                                
            }
        }
        stage('Test the build code')
        {
            steps
            {
                echo "${STAGE_NAME}"                                
            }
        }
        stage('Deploy the test code')
        {
            steps
            {
                echo "${STAGE_NAME}"                                
            }
        }
    }
}
